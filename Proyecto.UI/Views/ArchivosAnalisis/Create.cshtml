@model ArchivoAnalisis

<h2>Registrar Archivo de Análisis</h2>

<form asp-action="Create" method="post">
    <div class="form-group">
        <label>Nombre</label>
        <input asp-for="Nombre" class="form-control" />
        <span asp-validation-for="Nombre" class="text-danger"></span>
    </div>

    <div class="form-group">
        <label>Texto del Archivo</label>
        <textarea asp-for="TextoDelArchivo" class="form-control"></textarea>
        <span asp-validation-for="TextoDelArchivo" class="text-danger"></span>
    </div>

    <div class="form-group">
        <label>Fuente</label>
        <input asp-for="Fuente" class="form-control" />
        <span asp-validation-for="Fuente" class="text-danger"></span>
    </div>

    <button type="submit" class="btn btn-success">Guardar</button>
    <a asp-action="Index" class="btn btn-secondary">Volver</a>
</form>
